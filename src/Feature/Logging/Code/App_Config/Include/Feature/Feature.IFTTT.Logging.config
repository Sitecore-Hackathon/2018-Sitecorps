<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore>
    <log4net>
      <appender name="LogFileAppender" >
        <patch:attribute name="type">Community.Feature.IFTTT.Logging.Appender.IFTTTRollingFileAppender,Community.Feature.IFTTT.Logging</patch:attribute>
      </appender>
    </log4net>
    <settings>
      <setting name="IFTTT.Logging.Enabled" value="true" />
    </settings>
  </sitecore>
</configuration>